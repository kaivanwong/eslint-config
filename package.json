{
  "name": "@kaivanwong/eslint-config-monorepo",
  "version": "0.12.1",
  "packageManager": "pnpm@7.21.0",
  "description": "Kaivanwong's ESLint config presets",
  "author": "Kaivan Wong <kaivanwong@outlook.com>",
  "license": "MIT",
  "homepage": "https://github.com/kaivanwong/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaivanwong/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/kaivanwong/eslint-config/issues"
  },
  "keywords": [
    "kaivanwong",
    "eslint-config"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag && pnpm -r publish --access public"
  },
  "devDependencies": {
    "@kaivanwong/eslint-config": "workspace:*",
    "bumpp": "^8.2.1",
    "eslint": "^8.33.0"
  }
}
